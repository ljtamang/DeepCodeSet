public class MyTester {
    public static void main(String[] args) {
        int i = 7;
        int j = 10;
        int result;

        int k = j / i;
        result = k * 5 % 3;
        System.out.println(result);
    }
}
//answer
2
//split
public class MyTester {
    public static void main(String[] args) {

        int myYear = 2019;
        String myText = new String("Hello guys");
        int result = 0;

        if (myText.length() > 5) {
            result = 1;
            if (myText.length() < 30 && myYear >= 2018) {
                result += 5;
            }
        } else {
            if (myYear >= 2000) {
                result += 10;
            } else {
                result += 100;
            }
        }

        System.out.println(result);

    }
}
//answer
6
//split
public class MyTester {
    public static void main(String[] args) {
        int i = 5;
        int result = 0;
        while (i < 7) {
            result += i;
            i++;
        }

        System.out.println(result);
    }
}
//answer
11
//split
public class MyTester {
    public static void main(String[] args) {
        int result = 1;

        for (int i = 4; i > 0; i--) {
            result += i;
        }

        System.out.println(result);
    }
}
//answer
11
//split
public class MyTester {
    public static void main(String[] args) {
        int[] data = new int[6];

        for (int i = 0; i < 6; i++) {
            data[i] = i * i;
        }

        data[3] += 1;
        System.out.println(data[3]);

    }
}
//answer
10
//split
public class MyTester {
    public static void main(String[] args) {
        Rectangle myBox = new Rectangle(4, 6);
        myBox.magnify(2);
        result = myBox.getHeight() + myBox.getWidth()
        System.out.println(result);
    }
}

public class Rectangle {
    private double height;
    private double width;
    public Rectangle(double height, double width) {
        this.height = height;
        this.width = width;
    }
    public void magnify(int ratio) {
        height = height * ratio;
        width = width * ratio;
    }
}
//answer
20
