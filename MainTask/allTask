public class Tester {
    public static void main(String[] args) {
        int num = 25;
        int divisor = 2;
        while (num % divisor != 0) {
            divisor += 1;
        }
        System.out.println(divisor);
    }
}
//answer
5
//split
public class Tester {
    public static void main(String[] args) {
        int result = 2;
        for (int i = 0; i < 5; i++) {
            result = result + i;
        }
        System.out.println(result);
    }
}
//answer
